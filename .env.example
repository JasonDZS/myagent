# Copy this file to `.env` and adjust values as needed.
# Values mirror the defaults defined in `myagent/config.py`.

# OpenAI / LLM configuration
OPENAI_API_KEY=""
OPENAI_API_BASE="https://api.openai.com/v1"
LLM_MODEL="gpt-3.5-turbo"
LLM_MAX_TOKENS="2048"

# Embedding service
EMBEDDING_MODEL="Pro/BAAI/bge-m3"
EMBEDDING_PROVIDER="openai"
EMBEDDING_DIM="1024"
EMBEDDING_MAX_TOKENS="8192"

# SERPER_API_KEY for web search functionality
SERPER_API_KEY=""

# Markdown conversion toggles
MARKDOWN_PRESERVE_HTML="true"
MARKDOWN_INCLUDE_METADATA="true"
MARKDOWN_FLATTEN_STRUCTURE="false"
MARKDOWN_MAX_DEPTH="10"
MARKDOWN_OUTPUT_FORMAT="json"

# Concurrency controls
MAX_CONCURRENT="4"


# MYSQL configuration
MYSQL_HOST="localhost"
MYSQL_PORT="3306"
MYSQL_USER="dizs"
MYSQL_PASSWORD="123456"
MYSQL_DATABASE="fitbit_db"