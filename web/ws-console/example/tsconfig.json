{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["react", "react-dom"],
    "baseUrl": ".",
    "paths": {
      "@myagent/ws-console": ["../src"],
      "@myagent/ws-console/*": ["../src/*"]
    }
  },
  "include": ["src/**/*"]
}
